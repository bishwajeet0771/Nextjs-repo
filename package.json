{
  "name": "grp-front",
  "version": "0.1.0",
  "private": true,
  "homepage": "/react",
  "scripts": {
    "dev": "next dev",
    "start": "npm i && npm run build && cross-env NODE_ENV=production next start -p 3000",
    "test": "npm run build:test && cross-env NODE_ENV=test next start -p 3000",
    "build": "next build",
    "lint": "next lint",
    "build:test": "cross-env NODE_ENV=test next build",
    "test:unit": "set NODE_OPTIONS=--no-deprecation && jest --detectOpenHandles",
    "test:watch": "set NODE_OPTIONS=--no-deprecation && jest --watch",
    "push": "npm run build && git add . && read -p 'Enter commit message: ' msg && git commit -m \"$msg\" && git push",
    "debug": "cross-env NODE_OPTIONS='--inspect' next dev",
    "artifact": "node build.js",
    "pre-prod": "npm run build",
    "pre-prod-start": "cross-env NODE_ENV=production next start -p 3000",
    "staging": "cross-env ENVIRONMENT=staging  npm run build  && cross-env NODE_ENV=production next start -p 3000",
    "test-build": "cross-env NODE_ENV=test next build",
    "test-start": "cross-env NODE_ENV=test next start -p 3000",
    "prod-build": "npm run build",
    "prod-start": "next start -p 3000",
    "postbuild": "next-sitemap",
    "cypress:open": "cypress open"
  },
